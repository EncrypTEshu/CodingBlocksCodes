package Lecture2;
import java.util.*;
public class Pattern7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=sc.nextInt();
		int r=1;
		int val=0;
		int nsp=n-1;
		while(r<=10)
		{
			int csp=1;
			while(csp<=nsp) {
				System.out.print(" ");
			csp++;
			}
			
		nsp--;
		System.out.println();
		r++;
	
	}

}
}
