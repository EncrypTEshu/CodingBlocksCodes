package starter;

public class Candy {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			System.out.println(prime(11));
	}
	public static boolean prime(int n){
		int c=0;
		for(int i=1;i*i<=n;i++){
			if(n%i==0){
				c++;
			}
		}
		if(c==2)
		return true;
		else
		return false;
	}

}
